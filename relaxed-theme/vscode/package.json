{
    "name": "relaxed-dark",
    "displayName": "Relaxed Dark",
    "description": "A relaxing dark theme to make your daily coding pleasant for your eyes",
    "version": "0.3.4",
    "publisher": "TobiasDev",
    "icon": "images/marketplace-icon.png",
    "galleryBanner": {
        "color": "#121317",
        "theme": "dark"
      },   
	"repository": {
