        "themes": [
            {
                "label": "Relaxed-Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/relaxed-dark-color-theme.json"
            }
        ]
    },
    "devDependencies": {
	    "nodemon": "^1.14.12",
	    "vsce": "^1.36.2"
